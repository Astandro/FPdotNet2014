            <ImageBrush ImageSource="Assets/BG3.png"></ImageBrush>
        </Grid.Background>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <!--TitlePanel contains the name of the application and page title-->
        <StackPanel Grid.Row="0" Margin="12,17,0,28">
            <TextBlock Text="Detail Hasil Tes" Style="{StaticResource PhoneTextNormalStyle}"/>
            <TextBlock Text="Detail Hasil" Margin="9,-7,0,0" Style="{StaticResource PhoneTextTitle1Style}" FontSize="48" Height="61"/>
        </StackPanel>

